{
  "name": "new-react-transliterate",
  "version": "1.1.6",
  "description": "Transliterate component for React",
  "author": "kuna",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkunalan/react-transliterate"
  },
  "keywords": [
    "react",
    "transliterate",
    "tamil",
    "google input tools",
    "input",
    "component"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./styles.css": {
      "default": "./dist/new-react-transliterate.css"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "prepare": "vite build && tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "textarea-caret": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "^5.4.5",
    "vite": "^7.0.3"
  },
  "packageManager": "yarn@1.22.22"
}
